<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" configfailurepolicy="continue" parallel="classes" thread-count="2">
<listeners>
  <listener class-name="listeners.AnnotationTransformer"/>
<listener class-name= "listeners.TestListener"/>
</listeners>
  <test name="LandingPageTests">
   <parameter name="incognito" value="false"/>
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="tests.LandingPageTests"/>
    </classes>
  </test> <!-- Test -->
    <test name="DemoPageTests">
   <parameter name="incognito" value="false"/>
  <parameter name="browser" value="chrome"></parameter>
    <classes>
     <class name="tests.LandingPageTests"/>
      <class name="tests.DemoPageTests"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
